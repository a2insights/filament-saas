{
    "name": "a2insights/filament-saas",
    "description": "Filament Saas for A2Insights",
    "keywords": [
        "laravel",
        "filament",
        "a2insights",
        "filament-saas"
    ],
    "homepage": "https://github.com/a2insights/filament-saas",
    "support": {
        "issues": "https://github.com/a2insights/filament-saas/issues",
        "source": "https://github.com/a2insights/filament-saas"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "a21ns1g4ts",
            "email": "atila.danvi@outlook.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "amidesfahani/filament-tinyeditor": "^4.0",
        "andrewdwallo/filament-companies": "^5.0",
        "artesaos/seotools": "^1.3",
        "awcodes/filament-quick-create": "^4.0",
        "awcodes/palette": "^2.0",
        "bezhansalleh/filament-panel-switch": "^2.0",
        "bezhansalleh/filament-shield": "^4.0",
        "blade-ui-kit/blade-heroicons": "^2.6",
        "blade-ui-kit/blade-icons": "^1.8",
        "blade-ui-kit/blade-ui-kit": "^0.7.0",
        "blade-ui-kit/blade-zondicons": "^1.4",
        "codeat3/blade-iconpark": "^1.7",
        "filament/filament": "^4.0",
        "filament/notifications": "^4.0",
        "filament/spatie-laravel-media-library-plugin": "^4.0",
        "filament/spatie-laravel-settings-plugin": "^4.0",
        "filament/tables": "^4.0",
        "giggsey/libphonenumber-for-php": "9.0.13",
        "illuminate/contracts": "^12.0",
        "illuminate/support": "^12.0",
        "jeffgreco13/filament-breezy": "^3.0",
        "joaopaulolndev/filament-pdf-viewer": "^2.0",
        "kenepa/banner": "^1.0",
        "laravel/sanctum": "^4.2",
        "league/flysystem-aws-s3-v3": "^3.29",
        "livewire/livewire": "^3.6",
        "lorisleiva/laravel-actions": "^2.9",
        "malzariey/filament-daterangepicker-filter": "^4.0",
        "marjose123/filament-webhook-server": "^3.0",
        "opcodesio/log-viewer": "^3.19",
        "pxlrbt/filament-environment-indicator": "^3.1",
        "pxlrbt/filament-spotlight": "^2.0",
        "ralphjsmit/laravel-seo": "^1.7",
        "solution-forest/filament-field-group": "^2.0",
        "solution-forest/filament-firewall": "^3.0",
        "spatie/laravel-data": "^4.17",
        "spatie/laravel-enum": "^3.2",
        "spatie/laravel-package-tools": "^1.92",
        "spatie/laravel-sitemap": "^7.2",
        "spatie/laravel-stats": "^2.1",
        "stancl/tenancy": "^3.8",
        "symfony/intl": "^7.2",
        "taylornetwork/laravel-username-generator": "^2.8",
        "ysfkaya/filament-phone-input": "4.0"
    },
    "require-dev": {
        "laravel/pint": "^1.24",
        "nunomaduro/collision": "^8.2.2",
        "orchestra/testbench": "^10.6.5",
        "pestphp/pest": "^4.0",
        "pestphp/pest-plugin-arch": "^4.0",
        "pestphp/pest-plugin-laravel": "^4.0"
    },
    "autoload": {
        "files": [
            "src/helpers.php"
        ],
        "psr-4": {
            "A2Insights\\FilamentSaas\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "A2Insights\\FilamentSaas\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "cs": [
            "rector",
            "pint",
            "npm run prettier"
        ],
        "pint": "pint --config pint-strict-imports.json",
        "rector": "rector",
        "test:pest": "pest --parallel",
        "test:phpstan": "phpstan analyse",
        "test": [
            "@test:pest",
            "@test:phpstan"
        ]
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "A2Insights\\FilamentSaas\\FilamentSaasServiceProvider"
            ],
            "aliases": {
                "FilamentSaas": "A2Insights\\FilamentSaas\\Facades\\FilamentSaas"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}